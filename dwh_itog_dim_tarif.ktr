    <connection>db_bookings</connection>
    <sql>select distinct 	
		f.flight_id as fl_id,
		f.actual_departure::date as dt_start,
		f.aircraft_code as aircraft_code,
		f.departure_airport as airport_start,
		f.arrival_airport as airport_end,
		tf.fare_conditions as klass,
		tf.amount as amount 			
from bookings.flights f 
left join bookings.ticket_flights tf on tf.flight_id = f.flight_id 
where 
	f.status = 'Arrived' and 
	not tf.ticket_no is null and 
	f.actual_departure between ? and ?  
order by f.actual_departure::date,f.aircraft_code,
	f.departure_airport,f.arrival_airport,tf.fare_conditions
	</sql>
